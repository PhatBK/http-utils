{
  "name": "@truefit/http-utils",
  "version": "2.0.2",
  "description": "a handy wrapper around axios",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "format": "prettier --write **/*.{js,ts,tsx,json}",
    "lint": "eslint src/**/*.ts",
    "clean-build": "rm -rf ./build",
    "compile": "tsc -p .",
    "build": "yarn format && yarn lint && yarn clean-build && yarn compile",
    "prepublishOnly": "yarn build"
  },
  "browserslist": [
    "defaults"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TrueFit/http-utils.git"
  },
  "authors": [
    "jgretz",
    "adamkirkton",
    "nauticalcoder"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/TrueFit/http-utils/issues"
  },
  "homepage": "https://github.com/TrueFit/http-utils#readme",
  "dependencies": {
    "axios": "^0.19.2"
  },
  "devDependencies": {
    "@types/node": "^13.7.7",
    "@typescript-eslint/eslint-plugin": "^2.21.0",
    "@typescript-eslint/parser": "^2.21.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-typescript-prettier": "^1.4.2",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-prettier": "^3.1.2",
    "prettier": "^1.19.1",
    "ts-node": "^8.6.2",
    "typescript": "^3.8.3"
  }
}
